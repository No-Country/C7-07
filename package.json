  "main": "index.js",
  "scripts": {
    "server:fake": "json-server --watch ./web/api_fake/api_fake.json --port 3005",
    "web:dev": "cd web && npm run dev",
    "server:dev": "cd server && npm run dev",
    "server:add": "cd server && npm install",
    "web:add": "cd web && npm install",
    "server:remove": "cd server && npm uninstall",
    "web:remove": "cd web && npm uninstall",
    "lint:check:server": "npx eslint ./server",
    "lint:check:web": "npx eslint ./web",
    "lint:fix:server": "npx eslint --fix ./server --ext .js,.ts,.json",
    "lint:fix:web": "npx eslint --fix ./web --ext .js,.ts,.json,.jsx,.tsx"
  },
  "workspaces": [
    "server",
    "web"
  ],
